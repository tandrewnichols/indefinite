{
  "name": "indefinite",
  "description": "Prefix a noun with an indefinite article - a or an - based on whether it begins with a vowel",
  "version": "2.0.3",
  "main": "dist/indefinite.umd.js",
  "module": "dist/indefinite.es.js",
  "scripts": {
    "build": "rollup -c rollup.build.js",
    "pretest": "npm run build",
    "test": "gulp",
    "travis": "gulp ci",
    "cover": "nyc --require babel-register --reporter=html --reporter=lcov --reporter=text-summary mocha --require babel-register --timeout=3000 --reporter=dot test/**/*.js",
    "prepublish": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tandrewnichols/indefinite.git"
  },
  "keywords": [
    "article",
    "a",
    "an",
    "grammar",
    "noun"
  ],
  "author": {
    "name": "Andrew Nichols",
    "email": "tandrewnichols@gmail.com",
    "url": "www.tandrewnichols.me"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tandrewnichols/indefinite/issues"
  },
  "homepage": "https://github.com/tandrewnichols/indefinite",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "eslint-codeframe-formatter": "^1.0.2",
    "file-manifest": "^2.0.4",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-codeclimate-reporter": "github:tandrewnichols/gulp-codeclimate-reporter",
    "gulp-eslint": "^4.0.0",
    "gulp-rename": "^1.2.2",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-rollup-preprocessor": "^5.1.1",
    "mocha": "^4.0.1",
    "nyc": "^11.4.1",
    "opn": "^5.1.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.54.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.1.0"
  }
